<%# locals: data, id, height, xtitle, ytitle %>

<%= bar_chart data,
  id: id,
  height: height,
  xtitle: xtitle,
  ytitle: ytitle,
  library: {
    animation: {
      duration: 1500,
      easing: 'easeInQuad',
      delay: 0,
      loop: false
    },
    scales: {
      y: {
        ticks: {
          autoSkip: false
        }
      }
    },
    onClick: "function(event) {
      console.log('=== CHART WAS CLICKED ===')
    }"
  } %>
